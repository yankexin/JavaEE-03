<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.mybatis.dao.StuDao">
    <select id="findAll" resultType="com.example.mybatis.entity.student">select * from student</select>
    <insert id="addStudent" parameterType="student">insert into student(id,name,time) values(#{id},#{name},#{time})</insert>
</mapper>