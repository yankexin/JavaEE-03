<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.mybatis.dao.HomDao">
    <select id="findAll" resultType="com.example.mybatis.entity.homework">select * from homework</select>
    <insert id="addHomework" parameterType="homework">insert into homework(id,title,content,time) values(#{id},#{title},#{content},#{time})</insert>
</mapper>